<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>配送先名</th>
      <th>郵便番号</th>
      <th>住所</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% addresses.each do |address| %>
      <tr>
        <td><%= address.name %></td>
        <td><%= address.postal_code %></td>
        <td><%= address.address %></td>
      </tr>
    <% end %>
  </tbody>
</table>