<table class="table">
  <thead class="bg-light">
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
    <% orders.each do |order| %>
    <tr>
      <td><%= order.created_at.strftime('%Y/%m/%d/%H:%M:%S') %></td>
      <td><%= order.user.last_name %><%= order.user.first_name %></td>
      <td><%= @order.quantity %></td>
      <td><%= order.order_status %></td>
    </tr>
    <% end %>
  </thead>
</table>