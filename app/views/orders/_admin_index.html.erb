<table class="table">
  <thead class="bg-light">
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
    <%# orders.each do |order| %>
    <tr>
      <td><%#= orders.created_at.strftime('%Y/%m/%d/%h/%H/%M/%S') %></td>
      <td><%#= orders.user.last_name %><%#= orders.user.first_name %>フルネーム</td>
      <td><%#= orders.count %>カウント</td>
      <td><%#= orders_status %>ステータス</td>
    </tr>
    <%# end %>
  </thead>
</table>