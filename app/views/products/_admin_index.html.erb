<table class='table'>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% products.each do |product| %>
      <tr>
        <td><%= attachment_image_tag(product, :image, :fill, 50, 50, fallback: "no-image-icon.jpg") %></td>
        <td><%= product.product_name %></td>
        <td><%= product.non_taxed_price %></td>
        <td><%= product.category.category_name %></td>
        <td><%= product.production_status %></td>
      </tr>
    <% end %>
  </tbody>
</table>